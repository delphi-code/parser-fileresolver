{
    "name": "delphi/parser-fileresolver",
    "description": "PHPParser Visitor to add File node as parent node",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Sartain",
            "email": "tomsartain@gmail.com"
        }
    ],
    "require": {
        "nikic/php-parser": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "delphi\\Parser\\File\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "delphi\\Tests\\Parser\\File\\": "tests/"
        }
    }
}
